{
  "name": "react-map-gl-website",
  "scripts": {
    "start": "yarn clean && yarn develop",
    "build": "(cd .. && yarn build) && yarn clean-examples && yarn clean && gatsby build --prefix-paths",
    "clean": "rm -rf ./.cache ./public && yarn update-external-docs",
    "clean-examples": "find ../examples -name node_modules -exec rm -r {} \\; || true",
    "update-external-docs": "curl https://raw.githubusercontent.com/uber-web/math.gl/master/modules/web-mercator/docs/api-reference/web-mercator-viewport.md > ../docs/api-reference/web-mercator-viewport.md",
    "develop": "yarn clean-examples && gatsby develop",
    "serve": "gatsby serve",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -d public"
  },
  "dependencies": {
    "@globalfishingwatch/api-client": "1.2.5",
    "@globalfishingwatch/layer-composer": "2.4.4",
    "@globalfishingwatch/map-components": "^3.1.0",
    "@globalfishingwatch/react-hooks": "2.5.0",
    "@globalfishingwatch/timebar": "1.0.12",
    "@turf/area": "^6.0.1",
    "@turf/bbox": "^6.0.1",
    "babel-polyfill": "^6.26.0",
    "d3-array": "^1.0.5",
    "d3-request": "^1.0.5",
    "d3-scale": "^1.0.6",
    "dayjs": "^1.8.30",
    "gl-matrix": "^3.0.0",
    "immutable": "^3.7.5",
    "luxon": "^1.24.1",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-map-gl-draw": "^0.17.2",
    "styled-components": "^4.3.2",
    "mapbox-gl": "https://github.com/GlobalFishingWatch/mapbox-gl-js.git#build"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.14",
    "@mdx-js/react": "^1.6.14",
    "gatsby": "~2.24.12",
    "gatsby-plugin-env-variables": "^1.0.1",
    "gatsby-plugin-mdx": "^1.2.30",
    "gatsby-plugin-no-sourcemaps": "^2.2.0",
    "gatsby-theme-ocular": "^1.2.1",
    "gh-pages": "^2.1.0"
  }
}
